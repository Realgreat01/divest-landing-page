    "sass": "^1.63.3",
    "vee-validate": "^4.9.6",
    "vue": "^3.3.4",
    "vue-m-message": "^4.0.2",
    "yup": "^1.2.0"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.2.0",
    "@tsconfig/node18": "^2.0.1",
    "@types/node": "^18.16.17",
