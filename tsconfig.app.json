{
  "extends": "@vue/tsconfig/tsconfig.dom.json",
  "include": ["env.d.ts", "src/**/*", "src/**/*.vue", "prettier.config.cjs"],
  "exclude": ["src/**/__tests__/*"],
  "compilerOptions": {
    "baseUrl": ".",
    "target": "es2017",
    "module": "esnext",
    "lib": ["esnext", "dom"],
    "ignoreDeprecations": "5.0",
    "paths": {
      "@/*": ["./src/*"]
    },
    "typeRoots": ["./node_modules/@types", "./@types"]
  },
  "include": ["env.d.ts", "src/**/*", "src/**/*.vue", "src/vue-shims.d.ts", "public/data", "prettier.config.cjs"],
}
